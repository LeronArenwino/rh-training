{
    "request": {
      "method": "POST",
      "url": "/MailService.asmx",
      "headers": {
        "SOAPAction": {
          "matches": ".*ResendEmail.*"
        }
      },
      "bodyPatterns": [
        {
          "matches": ".*<emailId>ERROR</emailId>.*"
        }
      ]
    },
    "response": {
      "status": 500,
      "headers": {
        "Content-Type": "text/xml; charset=utf-8"
      },
      "body": "<?xml version=\"1.0\" encoding=\"utf-8\"?><soap:Envelope xmlns:soap=\"http://schemas.xmlsoap.org/soap/envelope/\"><soap:Body><soap:Fault><faultcode>soap:Server</faultcode><faultstring>Internal Server Error</faultstring></soap:Fault></soap:Body></soap:Envelope>"
    }
  }